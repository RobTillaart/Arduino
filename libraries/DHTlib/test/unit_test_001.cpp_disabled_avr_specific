#include <ArduinoUnitTests.h>


#include "Arduino.h"

#if defined (ARDUINO_ARCH_AVR)

#include "dht.h"

unittest_setup()
{
}

unittest_teardown()
{
}

unittest(test_constructor)
{
  assertEqual(1, 1);
}



unittest_main()

